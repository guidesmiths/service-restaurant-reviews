{
	"name": "service-restaurant-reviews",
	"version": "0.0.1",
	"description": "service API",
	"main": "index.js",
	"scripts": {
		"local": "SERVICE_ENV=local node index.js",
		"start": "node index.js",
		"db:up": "docker-compose --file ./docker/test/docker-compose.yml pull && docker-compose --file ./docker/test/docker-compose.yml up -d --force-recreate",
		"db:down": "docker-compose --file ./docker/test/docker-compose.yml down",
		"test:ci": "SERVICE_ENV=ci mocha --reporter spec './test/**/*.test.js'",
		"test:it": "npm run db:up && SERVICE_ENV=test mocha --reporter spec './test/integration/*.test.js' && npm run db:down",
		"test": "npm run db:up && sleep 2 && SERVICE_ENV=test mocha --reporter spec './test/**/*.test.js' && npm run db:down",
		"ci:up": "docker-compose --file ./docker/cicd/docker-compose.yml pull && docker-compose --file ./docker/cicd/docker-compose.yml up -d --build --force-recreate",
		"ci:down": "docker-compose --file ./docker/cicd/docker-compose.yml down",
		"lint": "node_modules/.bin/eslint .",
		"manifest": "node_modules/make-manifest/bin/make-manifest",
		"qa": "npm run lint && npm run test",
		"precommit": "npm run lint",
		"prepush": "npm run test",
		"postinstall": "node_modules/.bin/eslint . --fix"
	},
	"devDependencies": {
		"@commitlint/cli": "^13.2.1",
		"@commitlint/config-conventional": "^13.2.0",
		"chai": "^4.2.0",
		"eslint": "^6.5.1",
		"eslint-config-airbnb-base": "^11.3.2",
		"eslint-plugin-import": "^2.14.0",
		"eslint-plugin-mocha": "^5.2.0",
		"expect.js": "^0.3.1",
		"husky": "^7.0.4",
		"make-manifest": "^1.0.1",
		"mocha": "^5.2.0",
		"mocha-junit-reporter": "^1.18.0",
		"prettier": "^1.19.1",
		"sinon": "^7.3.2",
		"supertest": "^3.0.0"
	},
	"dependencies": {
		"axios": "^0.19.2",
		"body-parser": "^1.15.2",
		"boom": "^3.2.2",
		"bunyan": "^1.8.5",
		"chalk": "^1.1.3",
		"confabulous": "^2.1.0",
		"debug": "^2.2.0",
		"dotenv": "^8.2.0",
		"express-jsdoc-swagger": "^1.0.4",
		"handy-postgres": "^1.1.1",
		"hogan.js": "^3.0.2",
		"make-manifest": "^1.0.1",
		"on-headers": "^1.0.1",
		"optimist": "^0.6.1",
		"optional": "^0.1.3",
		"prepper": "^1.2.1",
		"ramda": "^0.26.1",
		"systemic": "^3.0.0",
		"systemic-domain-runner": "^1.1.0",
		"systemic-express": "^1.1.0"
	}
}
